from typing import Any, Optional

from ldap.controls import RequestControl, ResponseControl
from pyasn1.type import univ

class SortKeyType(univ.Sequence):
    componentType: Any = ...

class SortKeyListType(univ.SequenceOf):
    componentType: Any = ...

class SSSRequestControl(RequestControl):
    controlType: str = ...
    ordering_rules: Any = ...
    def __init__(self, criticality: bool = ..., ordering_rules: Optional[Any] = ...) -> None: ...
    def asn1(self): ...
    def encodeControlValue(self): ...

class SortResultType(univ.Sequence):
    componentType: Any = ...

class SSSResponseControl(ResponseControl):
    controlType: str = ...
    def __init__(self, criticality: bool = ...) -> None: ...
    sortResult: Any = ...
    attributeType: Any = ...
    result: Any = ...
    attribute_type_error: Any = ...
    def decodeControlValue(self, encoded: Any) -> None: ...
