from typing import Any, Optional

from ldap.controls import (
    KNOWN_RESPONSE_CONTROLS as KNOWN_RESPONSE_CONTROLS,
    LDAPControl as LDAPControl,
    RequestControl as RequestControl,
)

class AssertionControl(RequestControl):
    controlType: Any = ...
    criticality: Any = ...
    filterstr: Any = ...
    def __init__(self, criticality: bool = ..., filterstr: str = ...) -> None: ...
    def encodeControlValue(self): ...

class MatchedValuesControl(RequestControl):
    controlType: Any = ...
    criticality: Any = ...
    filterstr: Any = ...
    def __init__(self, criticality: bool = ..., filterstr: str = ...) -> None: ...
    def encodeControlValue(self): ...

class SimplePagedResultsControl(LDAPControl):
    controlType: Any = ...
    criticality: Any = ...
    def __init__(self, criticality: bool = ..., size: Optional[Any] = ..., cookie: Optional[Any] = ...) -> None: ...
    def encodeControlValue(self): ...
    def decodeControlValue(self, encodedControlValue: Any) -> None: ...
